<UserControl x:Class="ProjectCohesion.Win32.Controls.ComboBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjectCohesion.Win32.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             x:Name="thisControl">
    <local:AppXamlHost
        Width="{Binding Width,ElementName=thisControl}" 
        Height="Auto" 
        InitialTypeName="Windows.UI.Xaml.Controls.ComboBox"
        ChildChanged="AppXamlHost_ChildChanged"/>
</UserControl>
