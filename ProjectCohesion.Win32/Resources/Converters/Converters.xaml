<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:local="clr-namespace:ProjectCohesion.Win32.Resources.Converters">
    <local:GuidToControlConverter x:Key="GuidToControlConverter"/>
    <local:EqualsConverter x:Key="EqualsConverter"/>
    <local:EqualsConverter x:Key="EqualsConverterReverse" IsReverse="True"/>
    <local:ObjectToBoolConverter x:Key="ObjectToBoolConverter"/>
    <local:ObjectToBoolConverter x:Key="ObjectToBoolConverterReverse" IsReverse="True"/>
    <local:BoolToObjectConverter x:Key="ObjectToVisibilityConverter" TrueValue="Visible" FalseValue="Collapsed"/>
    <local:BoolToObjectConverter x:Key="ObjectToVisibilityConverterReverse" TrueValue="Collapsed" FalseValue="Visible"/>
</ResourceDictionary>