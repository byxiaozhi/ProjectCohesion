<Application x:Class="ProjectCohesion.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProjectCohesion" 
             StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ThemeResources xmlns="clr-namespace:ProjectCohesion.Win32.Resources;assembly=ProjectCohesion.Win32"/>
                <ResourceDictionary Source="/ProjectCohesion.Win32;component/Resources/Styles/Default.xaml"/>
                <ResourceDictionary Source="/ProjectCohesion.Win32;component/Resources/Converters/Converters.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
